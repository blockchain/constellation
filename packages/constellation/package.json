{
  "name": "@blockchain-com/constellation",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "rimraf dist && yarn tailwind:build && rollup -c",
    "lint:css": "stylelint --cache \"./src/**/*.{js,ts,tsx}\"",
    "lint:js": "eslint --cache \"./src/**/*.{js,ts,tsx}\"",
    "lint:staged": "lint-staged",
    "build:storybook": "build-storybook",
    "deploy:storybook": "yarn tailwind:build && yarn build:storybook && storybook-to-ghpages --existing-output-dir=./storybook-static",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:fix": "yarn lint:js --fix && yarn lint:css --fix",
    "storybook": "start-storybook -p 9009",
    "sb": "yarn storybook",
    "tailwind:build": "npx tailwindcss -i ./src/input.css -o ./src/components/styles.css",
    "tailwind:watch": "yarn tailwind:build --watch",
    "tw": "yarn tailwind:watch",
    "test": "jest",
    "test:update": "jest -u",
    "test:tsc": "yarn tsc --noEmit",
    "prepare": "cd .. && husky install constellation/.husky",
    "analyze_bundle:open": "yarn build --analyze_bundle",
    "analyze_bundle:json": "yarn build --analyze_bundle_json"
  },
